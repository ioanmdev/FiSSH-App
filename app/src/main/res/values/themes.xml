    <style name="Theme.FiSSH.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="Theme.FiSSH.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="Theme.FiSSH.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />
